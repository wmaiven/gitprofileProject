<%- include ('partials/header.ejs'); %>
<%- include ('partials/navbarhome.ejs'); %>

<body>
    <div class="container"> 
        <h1 style="color: orangered;">User Repositories</h1>
        <hr>
        <div class="card">
            <table class="table">
                <thead>
                    <tr>
                        <td>id </td>
                        <td>Repos Name</td>
                        <td>Url-Repos</td>
                        
                    </tr>
                </thead>
                <tbody>
                    <% data.forEach(datas =>{ %>
                        <tr>
                            <td><%=datas.id%></td>
                            <td><%=datas.name%> </td>
                            <td><%=datas.html_url%></td>
                        </tr>
                    <%})%> 
                </tbody>
            </table>
        </div> 
        <hr>
        <br>
    </div>    
</body>

